
<article class="media box">
<div id="<%= dom_id event %>">
  <div class="media-content"></div>
  <p>
    <strong>Title:  </strong>
    <%= event.title %>
  </p>  
  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Host:</strong>
    <%= event.creator.username %>
  </p>
  <p>
    <strong>Location:</strong>
    <%= event.location %>
  </p>
  <p>
    <strong>Date:</strong>
  <%= event.month %>/<%= event.day %>/<%= event.year %>
  </p>
</div>
</article>

